                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Test Hangar</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="active">@Html.ActionLink("Suites", "Index", "TestSuites")</li>
                    <li>@Html.ActionLink("Configurations", "Index", "Configurations")</li>
                    <li>@Html.ActionLink("Runs", "Index", "Runs")</li>
